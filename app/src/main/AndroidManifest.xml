        <activity android:name=".LoginSignup.MakeSelectionActivity" />
        <activity android:name=".LoginSignup.ForgotPasswordActivity" />
        <activity android:name=".LoginSignup.VerifyOtpActivity" />
        <activity android:name=".LoginSignup.Signup3Activity" />
        <activity android:name=".LoginSignup.Signup2Activity" />
        <activity android:name=".LoginSignup.Signup1Activity" />
        <activity android:name=".LoginSignup.LoginActivity" />
        <activity android:name=".LoginSignup.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>