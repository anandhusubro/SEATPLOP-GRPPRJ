        android:supportsRtl="true"
        android:theme="@style/Theme.SEATPLOP">
        <activity android:name=".MainCourse.TicketActivity"></activity>
        <activity android:name=".MainCourse.SeatStandActivity" />
        <activity android:name=".MainCourse.DateTimeActivity" />
        <activity android:name=".MainCourse.EventActivity" />
